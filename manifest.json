{
  "name": "jookmarkson",
  "version": "0.1",
  "description": "backup bookmarks in json",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "bookmarks",
    "downloads"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
